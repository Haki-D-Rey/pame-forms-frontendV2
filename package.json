{
  "name": "pame-frontend-v2",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "// -------- DESARROLLO --------": "-------------------------------------",
    "start": "expo start",
    "start:clean": "expo start -c",
    "start:devclient": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "android:debug": "cd android && ./gradlew installDebug",
    "android:release:apk": "cd android && ./gradlew assembleRelease",
    "android:release:aab": "cd android && ./gradlew bundleRelease",
    "// -------- BUILDS (EAS) --------": "-------------------------------------",
    "build:android:dev": "pnpm dlx eas-cli build -p android --profile development",
    "build:ios:dev": "pnpm dlx eas-cli build -p ios --profile development",
    "build:android:preview": "pnpm dlx eas-cli build -p android --profile preview",
    "build:ios:preview": "pnpm dlx eas-cli build -p ios --profile preview",
    "build:android:prod": "pnpm dlx eas-cli build -p android --profile production",
    "build:ios:prod": "pnpm dlx eas-cli build -p ios --profile production",
    "build:android:apk": "pnpm dlx eas-cli build -p android --profile internal-apk",
    "// (Opcional) SUBMIT A TIENDAS": "-------------------------------------",
    "submit:android": "pnpm dlx eas-cli submit -p android --latest",
    "submit:ios": "pnpm dlx eas-cli submit -p ios --latest",
    "// -------- CALIDAD / CI --------": "-------------------------------------",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --runInBand --coverage",
    "// -------- HEALTH CHECK --------": "-------------------------------------",
    "doctor": "pnpm dlx expo-doctor",
    "doctor:verbose": "pnpm dlx expo-doctor --verbose",
    "check:all": "pnpm run doctor && pnpm run lint && pnpm run typecheck && pnpm run test -- --passWithNoTests",
    "// -------- MANTENIMIENTO --------": "-------------------------------------",
    "clean": "git clean -Xfd && rm -rf node_modules android ios .expo .expo-shared && pnpm store prune",
    "reset-project": "node ./scripts/reset-project.js"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@hookform/resolvers": "^5.2.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-picker/picker": "^2.11.1",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "axios": "^1.13.1",
    "base-64": "^1.0.0",
    "expo": "~54.0.20",
    "expo-constants": "~18.0.10",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-linking": "~8.0.8",
    "expo-network": "^8.0.7",
    "expo-router": "~6.0.13",
    "expo-secure-store": "^15.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-web-browser": "~15.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.65.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/base-64": "^1.0.2",
    "@types/react": "~19.1.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "typescript": "~5.9.2"
  },
  "private": true
}