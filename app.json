{
  "expo": {
    "name": "Pame Formularios",
    "slug": "pame-frontend-v2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/pame-logo-t.png",
    "scheme": "pamefrontendv2",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.pamefrontendv2"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/pame-logo-t.png",
        "backgroundImage": "./assets/images/pame-logo-t.png",
        "monochromeImage": "./assets/images/pame-logo-t.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.pamefrontendv2"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6e5795ef-e243-4859-97fc-7f7cb738fdd7"
      }
    }
  }
}
